<script>
import pageHeader from '@/mixin/pageHeader'
import { userList } from '@/api'

export default {
  mixins: [
    pageHeader({title: '직원관리', groupName: '직원관리'})
  ],
  data () {
    return {
      memberList: [],
      isSelected: ''
    }
  },
  created () {
    this.memberFindList('test')
  },
  methods: {
    // api
    memberFindList (loginId) {
      userList(loginId)
        .then(res => {
          console.log(res.data)
          this.memberList = res.data
          this.memberList = this.noSort(this.memberList)
        })
        .catch(err => {
          console.log(err)
        })
    },
    noSort (list) {
      return list.slice().sort((a, b) => a.userNo < b.userNo ? 1 : -1)
    },
    selectInfoView (loginId, i) {
      this.isSelected = i
      console.log(loginId)
    }
  }
}
</script>

