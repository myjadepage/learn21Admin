import DateSelect from '@/components/parts/DateSelect'

let dateSelect = ({
  components: {
    DateSelect
  },
  data: function () {
    return {
      state: {
        startdate: '',
        enddate: ''
      }
    }
  },
  computed: {
    pfromDate () {
      return new Date()
    },
    ptoDate () {
      return new Date()
    }
  },
  methods: {
    startDate (date) {
      this.state.startdate = date
    },
    endDate (date) {
      this.state.enddate = date
    }
  }
})

export default dateSelect
