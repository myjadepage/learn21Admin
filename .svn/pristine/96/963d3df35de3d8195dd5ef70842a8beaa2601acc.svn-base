<script>
import datepicker from 'vuejs-datepicker'
import { ko } from 'vuejs-datepicker/dist/locale'
import { EventBus } from './eventBus'
import { phoneNum } from '@/assets/script'

export default {
  components: { datepicker },
  name: 'userDetail',
  data () {
    return {
      selectMember: {},
      saveMember: {},
      getCellNum: {},
      cellNum1: '',
      cellNum2: '',
      cellNum3: '',
      resignChecked: false,
      ko: ko
    }
  },
  created () {
    EventBus.$on('selecMemberInfo', item => {
      this.selectMember = this.getUserData(item)
    })
    EventBus.$on('clear', () => {
      this.clear()
    })
  },
  methods: {
    // 넘어온 직원정보 가공
    getUserData (data) {
      data.dateCreated = this.getFormatDate(data.dateCreated)
      this.getCellNum = phoneNum(data.cellNum)
      if (data.email === null) return false
      return data
    },
    saveCellNum1 (e) {
      this.cellNum1 = e.target.value
    },
    saveCellNum2 (e) {
      this.cellNum2 = e.target.value
    },
    saveCellNum3 (e) {
      this.cellNum3 = e.target.value
    },
    selectEmail (e) {
      console.log(e.target.value)
      this.saveMember.email = e.target.value
    },
    getFormatDate (date) {
      let temp = new Date(date)
      let year = temp.getFullYear()
      let month = temp.getMonth() + 1
      month = month > 9 ? month : `0${month}`
      let day = temp.getDate()
      day = day > 9 ? day : `0${day}`
      return `${year}-${month}-${day}`
    },
    memberSaveBtn () {
      // 저장할 직원정보 가공
      this.saveMember.cellNum = this.cellNum1 + this.cellNum2 + this.cellNum3
    },
    newMemberBtn () { // 신규버튼 클릭시
      this.selectMember = ''
      EventBus.$emit('selectClear')
    },
    clear () {
      this.selectMember = ''
    }
  },
  beforeDestroy () {
    EventBus.$off()
  }
}
</script>

